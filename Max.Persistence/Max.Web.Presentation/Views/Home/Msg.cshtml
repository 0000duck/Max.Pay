@model Max.Models.PushMessage.PushSystemMessage
@{
    Layout = null;
    string domain = "Domain".ValueOfAppSetting();
}


<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"
          name="viewport">
    @if (Model.IsDeleted == true)
    {
        <title>内容已删除</title>
    }
    else
    {
        <title>@Model.Title</title>
    }
    <style>
        img {
            max-width: 100%;
        }
    </style>
</head>
<body>

    @if (Model.IsDeleted == true)
    {
        @*<div style="width:100%;text-align:center;">内容已删除</div>*@
        <style type="text/css">
            body, img, p {
                margin: 0;
                padding: 0;
            }

            body {
                background: #efeff4;
            }

            .not-content img {
                display: block;
                margin: 100px auto 15px;
                width: 300px;
                max-width: 40%;
            }

            .not-content p {
                text-align: center;
                font-size: 16px;
                color: #999;
            }
        </style>
        <div class="not-content">
            <img src="data:image/png;base64," />
            <p>内容已删除</p>
        </div>
    }
    @*else if (Model.IsHidden == true)
        {
                <div style="width:100%;text-align:center;">内容已隐藏</div>
        }*@
    else
    {
        string shareUrl = domain + "/Home/Msg?id=" + Model.Id + "&share=true";

        <div id="EWallet_ShareItemInfo" style="display: none;">
            <ewallet_needshare>1</ewallet_needshare>
            <ewallet_sharetitle>@Model.Title</ewallet_sharetitle>
            <ewallet_sharedes>@Model.Summary</ewallet_sharedes>
            <ewallet_shareurl>@Html.Raw(shareUrl)</ewallet_shareurl>
            <ewallet_shareimagepath>@(domain)/Content/Images/qd_logo.png</ewallet_shareimagepath>
            <ewallet_needjump>1</ewallet_needjump>
            <ewallet_uniqueid></ewallet_uniqueid>
        </div>

        <div style="padding:10px;">
            <table>
                <tr>
                    <td style="font-size: 18px;">
                        <div style="padding-bottom: 10px; margin-bottom: 5px; border-bottom: 1px solid #e7e7eb;">@Model.Title</div>
                    </td>
                </tr>
                <tr>
                    <td style="font-size: 12px; color: gray;">
                        @Model.CreateTime.ToString("yyyy-MM-dd")
                    </td>
                </tr>
                @*<tr>
                    <td>
                        <img width="100%" src="@Model.ImagePath" />
                    </td>
                </tr>*@
                <tr>
                    <td id="content">
                        @Html.Raw(Model.Content)
                    </td>
                </tr>
                @if (ViewBag.AdsImageUrl != null)
                {
                    <tr id="shareImg" style="display:none;">
                        <td>
                            <a href="@(ViewBag.AdsLinkUrl)"><img width="100%" src="@(ViewBag.AdsImageUrl)" /></a>
                            <input type="hidden" name="name" value="@ViewBag.userAgent" />
                        </td>
                    </tr>
                }
            </table>
        </div>

        <script src="~/Scripts/zepto.min.js"></script>
        <script src="~/Scripts/jweixin-1.0.0.js"></script>
        <script src="~/Scripts/WxConfig.js"></script>
        <script src="~/Scripts/QQshare.js"></script>
        <script type="text/javascript">

            var currentShareUrl = '@Html.Raw(shareUrl)&v=' + RandomNumber();

            /// <summary>
            /// 产生随机数时间
            /// </summary>
            function RandomNumber() {
                var today = new Date();
                var num = Math.abs(Math.sin(today.getTime()));
                return num;
            };

            wxShareData = {
                title: '@Model.Title',
                desc: '@Model.Summary',
                link: currentShareUrl,
                imgUrl: '@(domain)/Content/Images/qd_logo.png',
                trigger: function (res) {
                    //alert('用户点击发送给朋友');
                },
                success: function (res) {
                    //alert('已分享');
                },
                cancel: function (res) {
                    //alert('已取消');
                },
                fail: function (res) {
                    //alert(JSON.stringify(res));
                }
            };

            //qq配置
            setShareInfo({
                title: '@Model.Title',
                summary: '@Model.Summary',
                pic: '@(domain)/Content/Images/qd_sharelogo.png',
                url: currentShareUrl,
                WXconfig: {
                    swapTitleInWX: true,
                    appId: wxCofig.appId, // 必填，公众号的唯一标识
                    timestamp: wxCofig.timestamp, // 必填，生成签名的时间戳
                    nonceStr: wxCofig.nonceStr, // 必填，生成签名的随机串
                    signature: wxCofig.signature // 必填，签名，见附录1
                }
            });

            function isApp() {
                var ua = navigator.userAgent.toLowerCase();
                return !!(/qdapp/.test(ua));
            }


            $(function () {
                var url = $("#EWallet_ShareItemInfo").find("ewallet_shareurl").html() + '&v=' + RandomNumber();
                $("#EWallet_ShareItemInfo").find("ewallet_shareurl").html(url);

                if (!isApp()) {
                    $('#shareImg').css("display", "block");
                }
            });
        </script>
    }

</body>
</html>
